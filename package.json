{
    "name": "rekrutteringsbistand-container",
    "version": "0.1.0",
    "private": true,
    "homepage": "/",
    "dependencies": {
        "@navikt/ds-css": "^0.16.14",
        "@navikt/ds-react": "^0.17.18",
        "@navikt/navspa": "^6.0.1",
        "amplitude-js": "^8.17.0",
        "loadjs": "^4.2.0",
        "react": "^18.1.0",
        "react-dom": "^18.1.0",
        "react-router-dom": "^6.3.0"
    },
    "scripts": {
        "start": "BROWSER=none react-scripts start",
        "start:import": "BROWSER=none REACT_APP_IMPORT=true REACT_APP_MOCK=true react-scripts start",
        "start:mock": "BROWSER=none REACT_APP_MOCK=true react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "prepare": "husky install"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "prettier"
        ]
    },
    "prettier": {
        "tabWidth": 4,
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 100
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version"
        ]
    },
    "devDependencies": {
        "@types/amplitude-js": "^8.16.0",
        "@types/express": "^4.17.13",
        "@types/loadjs": "^4.0.0",
        "@types/react": "^18.0.12",
        "@types/react-dom": "^18.0.5",
        "@types/react-router-dom": "^5.3.3",
        "eslint-config-prettier": "^7.2.0",
        "fetch-mock": "^9.11.0",
        "http-proxy-middleware": "^2.0.4",
        "husky": "^8.0.1",
        "prettier": "^2.2.1",
        "pretty-quick": "^3.1.3",
        "react-scripts": "^5.0.1",
        "typescript": "^4.7.4"
    }
}
